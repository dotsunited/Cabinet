{
    "name": "dotsunited/cabinet",
    "type": "library",
    "description": "Cabinet is a PHP 5.3+ library providing a simple file storage layer",
    "keywords": ["file", "storage", "amazon", "s3"],
    "homepage": "https://github.com/dotsunited/Cabinet",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Jan Sorgalla",
            "email": "jan.sorgalla@dotsunited.de",
            "homepage": "http://dotsunited.de"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8.35 || ~5.7 || ~6.4",
        "mikey179/vfsstream": "1.*",
        "aws/aws-sdk-php": "2.*"
    },
    "suggest": {
        "aws/aws-sdk-php": "For using the Amazon S3 adapter."
    },
    "autoload": {
        "psr-0": {
            "DotsUnited\\Cabinet": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.10-dev"
        }
    }
}
